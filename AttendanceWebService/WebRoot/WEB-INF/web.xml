<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>LogIn</servlet-name>
    <servlet-class>moz.service.LogIn</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ModifyPwd</servlet-name>
    <servlet-class>moz.service.ModifyPwd</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SignIn</servlet-name>
    <servlet-class>moz.service.SignIn</servlet-class>
    </servlet>
      <servlet>
        <servlet-name>GetName</servlet-name>
    <servlet-class>moz.service.GetName</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ReceiveQRCode</servlet-name>
    <servlet-class>moz.service.ReceiveQRCode</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>AttendanceSer</servlet-name>
    <servlet-class>moz.service.AttendanceSer</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>LogIn</servlet-name>
    <url-pattern>/LogIn</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>GetName</servlet-name>
    <url-pattern>/GetName</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>ModifyPwd</servlet-name>
    <url-pattern>/ModifyPwd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SignIn</servlet-name>
    <url-pattern>/SignIn</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReceiveQRCode</servlet-name>
    <url-pattern>/ReceiveQRCode</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AttendanceSer</servlet-name>
    <url-pattern>/AttendanceSer</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
